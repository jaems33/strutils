  "repository": {
    "type": "git",
    "url": "https://github.com/jaems33/strutils.git"
  },
  "keywords": [
    "string",
    "utility"
  ],
  "author": "James Teow <james.teow33@gmail.com> (http://www.jamesteow.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaems33/strutils/issues"
  },
  "homepage": "https://github.com/jaems33/strutils#readme",
  "devDependencies": {
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "ghooks": "^2.0.4",
    "jest": "^26.6.3",
    "semantic-release": "^17.2.4"
  },
  "config": {
    "commitzen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  }

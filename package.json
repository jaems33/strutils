{
  "name": "@jaems/strutils",
  "version": "1.1.0-beta.5",
  "description": "Small utility algorithms for strings.",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
